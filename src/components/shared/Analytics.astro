---
const isProd = import.meta.env.PROD;
---

{ isProd && (
  <script async is:inline src="https://www.googletagmanager.com/gtag/js?id=G-ELMF6V23GD"></script>
  <script is:inline>
  // @ts-expect-error
  window.dataLayer = window.dataLayer || [];
  function gtag() {
    // @ts-expect-error
    dataLayer.push(arguments);
  }
  // @ts-expect-error
  gtag('js', new Date());
  // @ts-expect-error
  gtag('config', 'G-ELMF6V23GD');
</script>
)
}